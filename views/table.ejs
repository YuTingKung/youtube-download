<table class="table table-striped table-bordered table-sm">
    <thead>
        <tr>
            <th>Name</th>
            <th>Operate</th>
        </tr>
    </thead>
    <tbody>
        <% shortUrls.forEach(shortUrl => { %> 
            <tr>
                <td style="word-wrap:break-word;"><a href="<%= shortUrl.fullUrl %>" target="_blank"><%= shortUrl.full %></a></td>
                <td>
                    <a href="<%= shortUrl.path %>" download="<%= shortUrl.name %>"><button class="copy btn-success btn-sm">Download</button></a>
                </td>
            </tr>
        <% }) %>  
    </tbody>
</table>

<img src="<%= shortUrls[0].thumbnail %>" alt="Youtube thumbnail" style="display: block;
margin-left: auto;
margin-right: auto;
margin-bottom: 50px;
width: 100%;">

